FoamFile
{
    version     2.0;
    format      ascii;
    class       surfaceScalarField;
    location    "0";
    object      alphaQGD;
}
// * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * * //

dimensions      [0 0 0 0 0 0 0];

internalField   uniform 0.5;

boundaryField
{
    "(inlet).*"
    {
        type            calculated;
        value           uniform 0;
    }
    
    "(outlet).*"
    {
        type            calculated;
        value           uniform 0;
    }
    
    #include "constr.include"
    
}


